---
permalink: /debug-feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>Debug Feed</title>
    <link>{{ metadata.url }}/blog/</link>
    <description>Test</description>
    <language>it-it</language>

    {% for post in collections.posts | slice(0, 2) %}
    <item>
      <title>{{ post.data.title }}</title>
      <link>{{ metadata.url }}{{ post.url }}</link>
      <guid>{{ metadata.url }}{{ post.url }}</guid>
      <pubDate>{{ post.date | dateToRfc3339 }}</pubDate>
      <description><![CDATA[{{ post.templateContent | htmlToAbsoluteUrls(metadata.url + post.url) | safe }}]]></description>    </item>
    {% endfor %}
  </channel>
</rss>

