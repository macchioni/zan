---
layout: layouts/base.njk
title: Links
---

<section class="show-link-underlines">
  <div class="flex-between">
    <h1>Links</h1>
    <span><a class="rss-feed" href="/links/feed.xml">Links RSS</a></span>
  </div>

  {% for link in collections.links %}
    <article>
      <p><span>&#10042; 132</span></p><br>
      <h3><a href="{{ link.data.url }}">{{ link.data.title }}</a></h3>
      <div class="description">
        {{ link.templateContent | safe }}
      </div>
    </article>
  {% endfor %}
</section>